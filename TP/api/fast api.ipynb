   "source": [
    "from fastapi import FastAPI\n",
    "\n",
    "app = FastAPI()\n",
    "\n",
    "\n",
    "@app.get(path=\"/\")\n",
    "async def root():\n",
    "    return {\"message\": \"Hello World :D\"}\n",
    "\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    import uvicorn\n",
    "\n",
    "    config = uvicorn.Config(app=app)\n",
    "    server = uvicorn.Server(config=config)\n",
    "    await server.serve()\n"
   ]
  }
